        content += '<tr><td><button>投票</button></td></tr>';
        content += '</table>';
        content += '</div>';

        $gallery.append(content);
      });

      // $("#conent").append($gallery);

      $('#btn_vote').click(function () {

        var jsonData = {"data": "123456"};

        $.ajax({
          url:      'user_vote/',
          async:    true,
          dataType: "json",
          data:     jsonData,
          type:     'POST',
          timeout:  12000, //12s time out
